  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngduc/syntax.codes.git"
  },
  "scripts": {
    "e2e": "start-server-and-test 'api-now -s ./dist' 3003 'jest'",
